<Window x:Class="AccTrade.View.AdminView.DeleteRole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccTrade.View.AdminView"
        mc:Ignorable="d"
                Title="Delete" Height="200" Width="200"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="GameName_tb" HorizontalAlignment="Center" Margin="0,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" PreviewKeyDown="GameName_tb_PreviewKeyDown" PreviewTextInput='GameName_tb_PreviewTextInput'/>
        <Label Content="Id:" HorizontalAlignment="Left" Margin="37,52,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Delete_btn" Content="Delete" HorizontalAlignment="Center" Margin="0,130,0,0" VerticalAlignment="Top" Height="35" Width="78" Click="Add_btn_Click"/>

    </Grid>
</Window>
