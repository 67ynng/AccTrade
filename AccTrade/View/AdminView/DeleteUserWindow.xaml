<Window x:Class="AccTrade.View.AdminView.DeleteUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccTrade.View.AdminView"
        mc:Ignorable="d"
        Title="Delete" Height="200" Width="200" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="UserId_tb" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="0,74,0,0" PreviewTextInput="UserId_tb_PreviewTextInput" PreviewKeyDown="UserId_tb_PreviewKeyDown"/>
        <Button x:Name="Delete_btn" Content="Delete User" HorizontalAlignment="Center" Margin="0,143,0,0" VerticalAlignment="Top" Click="Delete_btn_Click"/>
        <Label Content="Id:" HorizontalAlignment="Left" Margin="40,53,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
